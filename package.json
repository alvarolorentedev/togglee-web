    "classnames": "^2.2.6",
    "date-fns": "^2.17.0",
    "emoji-mart": "^3.0.0",
    "graphql": "^15.5.0",
    "graphql.macro": "^1.4.2",
    "js-cookie": "^2.2.0",
    "prop-types": "^15.7.2",
    "react": "^16.14.0",
    "react-cropper": "^2.1.4",
    "react-dom": "^16.14.0",
    "react-dropzone": "^11.3.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.2",
    "recharts": "^2.0.4",
    "workbox-background-sync": "^6.1.0",
    "workbox-broadcast-update": "^6.1.0",
    "workbox-cacheable-response": "^6.1.0",
    "workbox-core": "^6.1.0",
    "workbox-expiration": "^6.1.0",
    "workbox-google-analytics": "^6.1.0",
    "workbox-navigation-preload": "^6.1.0",
    "workbox-precaching": "^6.1.0",
    "workbox-range-requests": "^6.1.0",
    "workbox-routing": "^6.1.0",
    "workbox-strategies": "^6.1.0",
    "workbox-streams": "^6.1.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.6.2",
    "gh-pages": "^3.1.0"
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start": "REACT_APP_SERVICE_URL=http://localhost:8080 REACT_APP_SERVICE_USERNAME=admin REACT_APP_SERVICE_PASSWORD=supersecret react-scripts start",
    "build": "REACT_APP_SERVICE_URL=https://togglee.herokuapp.com REACT_APP_SERVICE_USERNAME=admin REACT_APP_SERVICE_PASSWORD=supersecret react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "test:coverage": "CI=true npm test -- --coverage --collectCoverageFrom=src/**/*.{js,jsx} --collectCoverageFrom=!src/**/*.stories.js",
    "report:complexity": "cr -c .complexrc src"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dunky11/react-saas-template.git"
  },
